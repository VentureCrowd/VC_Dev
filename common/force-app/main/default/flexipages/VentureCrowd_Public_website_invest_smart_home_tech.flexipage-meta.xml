            <componentInstance>
                <componentInstanceProperties>
                    <name>richTextValue</name>
                    <value>&lt;div class=&quot;content-container--html-editor&quot;&gt;

&lt;br&gt;

    &lt;h2&gt;How smart homes benefit consumers&lt;/h2&gt;
   
    &lt;/div&gt;</value>
                </componentInstanceProperties>
                <componentName>forceCommunity:htmlBlock</componentName>
