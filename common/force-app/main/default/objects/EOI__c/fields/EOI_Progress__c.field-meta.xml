<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>EOI_Progress__c</fullName>
    <description>Formula to evaluate Progress % of the EOI</description>
    <formula>IF( Target_Raise_Amount__c &gt; 0,
MIN(( Value_of_investment_interest__c / Target_Raise_Amount__c ), 1),
0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Formula to evaluate Progress % of the EOI</inlineHelpText>
    <label>EOI Progress</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
