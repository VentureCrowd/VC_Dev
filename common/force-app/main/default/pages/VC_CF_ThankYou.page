  <!-- VEN-152 - pass height of form to parent onload and onresize -->
  <script>
    postToParent = () => {
      let height = document.body.scrollHeight;
      window.parent.postMessage({ 'scrollHeight' : height }, '*');
    }

    window.onload = () => {
      postToParent();
    }

    window.addEventListener('resize', postToParent);
  </script>
