        <locationY>431</locationY>
        <inputAssignments>
            <field>Type__c</field>
            <value>
                <stringValue>Call - Voicemail</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Update_call_Inbound</name>
        <label>Update call Inbound</label>
        <locationX>50</locationX>
        <locationY>431</locationY>
        <inputAssignments>
            <field>Type__c</field>
            <value>
                <stringValue>Call - Inbound</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <recordUpdates>
        <name>Update_Call_Outbound</name>
        <label>Update Call Outbound</label>
        <locationX>314</locationX>
        <locationY>431</locationY>
        <inputAssignments>
            <field>Type__c</field>
