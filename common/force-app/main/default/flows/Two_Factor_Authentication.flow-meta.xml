<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionCalls>
        <name>Navigate_to_My_Portfolio</name>
        <label>Navigate to My Portfolio</label>
        <locationX>176</locationX>
        <locationY>434</locationY>
        <actionName>c:navigateToURL</actionName>
        <actionType>component</actionType>
        <flowTransactionModel>CurrentTransaction</flowTransactionModel>
        <nameSegment>c:navigateToURL</nameSegment>
    </actionCalls>
    <apiVersion>51.0</apiVersion>
    <areMetricsLoggedToDataCloud>false</areMetricsLoggedToDataCloud>
    <decisions>
        <description>This decision will decide if the user has already verified &amp; mobile number exitis</description>
        <name>Bypass_Login_Flow_OR_Not</name>
        <label>Bypass Login Flow OR Not</label>
        <locationX>176</locationX>
        <locationY>242</locationY>
        <defaultConnector>
            <targetReference>Navigate_to_My_Portfolio</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>ByPass 2FA</defaultConnectorLabel>
        <rules>
            <name>Proceed_With_2FA</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>UserMobile</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>twofactor</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Navigate_to_My_Portfolio</targetReference>
            </connector>
            <label>Proceed With 2FA</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>ForTLoggedInUserEmail</name>
        <dataType>String</dataType>
        <expression>{!$User.Email}</expression>
    </formulas>
    <formulas>
        <name>SMSMessage</name>
        <dataType>String</dataType>
        <expression>&quot;Hi &quot;  +  {!$User.FirstName} + &quot; &quot;  + &quot;, please use &quot; + TEXT( {!varNSecurityCode})  + &quot; to verify login access to VentureCrowd Investor Portal.&quot;</expression>
    </formulas>
    <formulas>
        <name>VarNCode</name>
        <dataType>Number</dataType>
        <expression>Value(SUBSTITUTE(LEFT(RIGHT(text( {!$Flow.CurrentDateTime} ),9),8), &quot;:&quot;, &quot;&quot; ))</expression>
        <scale>0</scale>
    </formulas>
    <interviewLabel>Two Factor Authentication {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Two Factor Authentication</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
